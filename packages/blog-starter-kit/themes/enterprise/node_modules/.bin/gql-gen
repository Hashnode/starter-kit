#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@parcel+watcher@2.3.0_@types+node@18.17.17_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/cli/cjs/node_modules:/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@parcel+watcher@2.3.0_@types+node@18.17.17_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/cli/node_modules:/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@parcel+watcher@2.3.0_@types+node@18.17.17_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/node_modules:/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@parcel+watcher@2.3.0_@types+node@18.17.17_graphql@16.8.0_typescript@5.2.2/node_modules:/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@parcel+watcher@2.3.0_@types+node@18.17.17_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/cli/cjs/node_modules:/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@parcel+watcher@2.3.0_@types+node@18.17.17_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/cli/node_modules:/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@parcel+watcher@2.3.0_@types+node@18.17.17_graphql@16.8.0_typescript@5.2.2/node_modules/@graphql-codegen/node_modules:/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@parcel+watcher@2.3.0_@types+node@18.17.17_graphql@16.8.0_typescript@5.2.2/node_modules:/Users/sandeeppanda/Documents/works/hashnode-starter-kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/cjs/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/cjs/bin.js" "$@"
fi
