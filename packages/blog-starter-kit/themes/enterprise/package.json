{
	"name": "@starter-kit/blog-enterprise",
	"private": true,
	"scripts": {
		"build": "cross-env GENERATE_SOURCEMAP=false next build",
		"codegen": "graphql-codegen --config codegen.yml",
		"dev": "concurrently \"next\" \"pnpm codegen --watch\" --names next,codegen",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\" --ignore-path ../../../../.prettierignore",
		"lint": "next lint --dir src",
		"start": "next start",
		"build-start": "pnpm run build && pnpm run start",
		"typecheck": "tsc",
		"lint:fix": "next lint --dir src --fix"
	},
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "7.18.6",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@starter-kit/utils": "workspace:*",
		"cryptly": "^4.0.5",
		"dompurify": "^3.1.6",
		"gray-matter": "^4.0.3",
		"js-cookie": "^3.0.5",
		"lodash": "^4.17.21",
		"lucide-react": "^0.436.0",
		"next": "^14.2.9",
		"react-google-recaptcha": "^3.1.0",
		"react-google-recaptcha-v3": "^1.10.1",
		"react-lazy-load-image-component": "^1.6.2",
		"react-waypoint": "^10.3.0",
		"web-vitals": "^4.2.3"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/typed-document-node": "^5.0.9",
		"@graphql-codegen/typescript": "^4.0.9",
		"@graphql-codegen/typescript-operations": "^4.2.3",
		"@parcel/watcher": "^2.4.1",
		"@radix-ui/react-dialog": "^1.1.1",
		"@radix-ui/react-dropdown-menu": "^2.1.1",
		"@radix-ui/react-popover": "^1.1.1",
		"@radix-ui/react-scroll-area": "^1.1.0",
		"@starter-kit/eslint-config-custom": "workspace:*",
		"@starter-kit/tsconfig": "workspace:*",
		"@tailwindcss/typography": "^0.5.15",
		"@types/js-cookie": "^3.0.6",
		"@types/lodash": "^4.17.7",
		"@types/node": "^20.16.5",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@types/uuid": "^10.0.0",
		"@vercel/og": "^0.6.2",
		"autoprefixer": "^10.4.20",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"date-fns": "^3.6.0",
		"eslint": "^9.10.0",
		"graphql-request": "^7.1.0",
		"html-react-parser": "^5.1.15",
		"postcss": "^8.4.45",
		"remark": "^15.0.1",
		"remark-html": "^16.0.1",
		"tailwind-merge": "^2.5.2",
		"uuid": "^10.0.0"
	}
}
